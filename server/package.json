{
  "name": "taha-world-server",
  "version": "1.0.0",
  "description": "Backend server for Taha World Sports Training Platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "seed": "node src/scripts/seed.js",
    "test:db": "node src/scripts/test-connection.js",
    "test:api": "node src/scripts/test-api.js",
    "test:integration": "npm run test:db && npm run test:api",
    "verify": "npm run test:db && npm run seed && npm run test:api",
    "build": "cd .. && cd client && npm install && npm run build"
  },
  "dependencies": {
    "@paypal/paypal-server-sdk": "^1.1.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "sports",
    "training"
  ],
  "author": "Taha World",
  "license": "MIT"
}
